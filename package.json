{
  "name": "smart-city-booking-vue-app",
  "description": "A VueJS Frontend for the Smart City Booking System",
  "version": "2.3.1",
  "private": true,
  "scripts": {
    "start": "node --max_old_space_size=4096 node_modules/@vue/cli-service/bin/vue-cli-service.js serve --mode production",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && mkdir -p ./dist/cdn/current && uglifyjs ./src/js-web-interface/booking-manager-js.js > ./dist/cdn/current/booking-manager.min.js",
    "test-build": "mkdir -p ./public/cdn/current && uglifyjs ./src/js-web-interface/booking-manager-js.js > ./public/cdn/current/booking-manager.min.js",
    "release:major": "npm version $(semver $npm_package_version -i major)",
    "release:minor": "npm version $(semver $npm_package_version -i minor)",
    "release:patch": "npm version $(semver $npm_package_version -i patch)"
  },
  "dependencies": {
    "@babel/core": "^7.23.6",
    "@mdi/font": "^7.3.67",
    "@tiptap/extension-bold": "^2.1.13",
    "@tiptap/extension-italic": "^2.1.13",
    "@tiptap/extension-paragraph": "^2.1.13",
    "@tiptap/extension-underline": "^2.1.13",
    "@tiptap/starter-kit": "^2.1.13",
    "@tiptap/vue-2": "^2.1.13",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-plugin-router": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "axios": "^1.6.2",
    "core-js": "^3.34.0",
    "date-fns": "^3.0.1",
    "loadash": "^1.0.0",
    "lodash": "^4.17.21",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "uglify-js": "^3.17.4",
    "uuid": "^9.0.1",
    "vee-validate": "^3.4.14",
    "vue": "^2.7.15",
    "vue-axios": "^3.5.2",
    "vue-cli-plugin-vuetify": "~2.5.1",
    "vue-i18n": "^8.28.2",
    "vue-router": "^3.6.5",
    "vue2-filters": "^0.14.0",
    "vuetify": "^2.6.8",
    "vuetify-loader": "^1.9.2",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.9",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.3.0",
    "prettier": "2.8.8",
    "semver": "^7.5.0",
    "vue-template-compiler": "^2.7.8"
  }
}
